{
  "manifest_version": 2,
  "name": "vue-chrome",
  "description": "A starting point for Chrome extensions using Vue.js",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "0.1.0",
  "browser_action": {
    "default_popup": "dist/popup/layout.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
